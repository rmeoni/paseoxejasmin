   		<script src="/CombinedSCroll/jquery.combinedScroll.js"></script>
   		<!--scrolling nav-->
   		<script type="text/javascript">
				jQuery(document).ready(function($) {
 
  					// Hook up the current state to the nav bar
  				$('.nav').onePageNav();
  				$('.page-navigation').onePageNav();
  				$('.mobile-menu').onePageNav();
  				
  
				});
				</script>
				<!--appear and disappear scrolling nav bar-->
				<script type="text/javascript">
			$(document).ready(function() {
   		    $(".site-navigation").hide(); //hide your div initially
    		var topOfOthDiv = $(".supporting").offset().top;
  
    		$(window).scroll(function() {
        	if($(window).scrollTop() >= topOfOthDiv) { //scrolled past the other div?
            $(".site-navigation").show(100); //reached the desired point -- show div
            
        		} else {
        			$(".site-navigation").hide(50);
        		}
   			 });
			});
		</script>

